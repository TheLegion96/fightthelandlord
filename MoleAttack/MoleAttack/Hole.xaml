<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	xmlns:local="clr-namespace:MoleAttack"
	x:Class="MoleAttack.Hole">

	<Grid x:Name="LayoutRoot">
		<Ellipse Stroke="Black">
			<Ellipse.Fill>
				<LinearGradientBrush EndPoint="0.808,0.269" StartPoint="0.037,0.466">
					<GradientStop Color="#FF8D4601" Offset="0"/>
					<GradientStop Color="#FFCC6600" Offset="1"/>
				</LinearGradientBrush>
			</Ellipse.Fill>
		</Ellipse>
		<Ellipse Fill="#FF5F2F01" Stroke="Black" StrokeThickness="0" Margin="26,27,24,8"/>
		<local:Mouse x:Name="mouse" Margin="-18,-110,-14,23"/>
	</Grid>
</UserControl>