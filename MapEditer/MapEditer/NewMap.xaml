<Window
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Class="MapEditor.NewMap"
	x:Name="Window"
	Title="NewMap"
	Width="274" Height="116">

	<Grid x:Name="LayoutRoot">
		<TextBlock HorizontalAlignment="Left" Margin="43.334,8.667,0,0" VerticalAlignment="Top" Width="45.334" Height="14.666" Text="地图名:" TextWrapping="Wrap"/>
		<TextBox x:Name="tbMapName" Margin="106.001,5.667,61.999,0" VerticalAlignment="Top" Height="17.666" Text="" TextWrapping="Wrap"/>
		<TextBlock Margin="43.334,30.333,0,37.001" TextWrapping="Wrap" HorizontalAlignment="Left" Width="58.667"><Run Text="地图图片"/><Run Text=":"/></TextBlock>
		<TextBox x:Name="tbImagePath" Margin="106.001,27.333,62,37.001" Text="" TextWrapping="Wrap" AllowDrop="True" IsUndoEnabled="True" IsTabStop="True" IsReadOnly="False" IsHitTestVisible="True" />
		<Button x:Name="btnBrowser" HorizontalAlignment="Right" Margin="0,27.333,42.666,37.001" Width="19.334" Content="..." FontSize="9.333" Click="btnBrowser_Click"/>
		<Button x:Name="btnOk" HorizontalAlignment="Left" Margin="53.333,0,0,8" VerticalAlignment="Bottom" Width="64.667" Height="22.667" Content="确定" Click="btnOk_Click"/>
		<Button x:Name="btnCancel" HorizontalAlignment="Right" Margin="0,0,61.999,8" VerticalAlignment="Bottom" Width="64.667" Height="22.667" Content="取消" Click="btnCancel_Click"/>
	</Grid>
</Window>